<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ljc.mapper.StoryStageConfigMapper">

    <select id="selectByCivAndStage" resultType="ljc.entity.StoryStageConfigTbl">
        SELECT * FROM story_stage_config 
        WHERE civ = #{civ} AND stage_no = #{stageNo}
        LIMIT 1
    </select>

</mapper>
